    "@typescript-eslint/parser": "^2.34.0",
    "chai": "^4.2.0",
    "commander": "^2.20.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^7.2.0",
    "mocha-logger": "^1.0.6",
    "mocha-prepare": "^0.1.0",
    "mongo-unit": "^2.0.1",
    "nodemon": "^2.0.4",
