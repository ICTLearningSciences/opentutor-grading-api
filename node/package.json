    "dotenv": "^8.2.0",
    "dotenvenc": "^1.0.4",
    "es6-promisify": "^6.1.1",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-handlebars": "^3.1.0",
    "express-validator": "^5.3.1",
    "graphql": "^14.6.0",
    "install": "^0.13.0",
    "is-hex": "^1.1.3",
    "iso8601-duration": "^1.2.0",
    "longjohn": "^0.2.12",
    "mongoose": "^5.9.20",
    "mongoose-cursor-pagination": "^1.0.1",
    "morgan": "^1.10.0",
    "node": "^13.14.0",
    "npm": "^6.14.5",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "serve-favicon": "^2.5.0",
    "tincanjs": "^0.50.0",
    "uuid": "^3.4.0",
